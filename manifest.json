{
  "manifest_version": 3,
  "name": "Modi-fy SIH",
  "version": "1.0",
  "description": "Do you have it in you.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.sih.gov.in/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Image Replacer"
  },
  "icons": {
    "128": "img/modiji.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["img/*"],
      "matches": ["https://www.sih.gov.in/*"]
    }
  ]
}